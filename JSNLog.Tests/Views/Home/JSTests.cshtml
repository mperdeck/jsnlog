    new T(3000, "", 91, logObject: "function() { return function() { return {\"f1\": \"v1b\", \"f2\": \"v2b\"}; }; }", expectedMsg: "{\"f1\":\"v1b\",\"f2\":\"v2b\"}"),
    
    new T(checkNbr: 91, checkAppender: "a0"),
    
