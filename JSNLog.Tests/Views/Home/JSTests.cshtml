    new T(3000, "", 91, logObject: "true", expectedMsg: "true"),
    new T(3000, "", 91, logObject: "new Boolean(false)", expectedMsg: "false"),
    new T(3000, "", 91, logObject: "undefined", expectedMsg: "undefined"),
